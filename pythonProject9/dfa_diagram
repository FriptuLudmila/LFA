	q3 [shape=doublecircle]
	"q3,q0" [shape=doublecircle]
	q0 [shape=circle]
	q1 [shape=circle]
	q2 [shape=circle]
	q0 -> q1 [label=a]
	q0 -> q2 [label=b]
	q2 -> "q3,q0" [label=c]
	"q3,q0" -> q1 [label=a]
	"q3,q0" -> q2 [label=b]
	q1 -> q3 [label=a]
	q1 -> q2 [label=b]
	node [shape=plaintext style=invisible]
	start [style=invisible]
	start -> q0 [style=dashed]
}
